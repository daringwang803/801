 /dts-v1/;
 
#include <dt-bindings/interrupt-controller/arm-gic.h>
#include <dt-bindings/input/input.h>
#include <dt-bindings/gpio/gpio.h>
#include <dt-bindings/pinctrl/pinctrl-leo.h>
#include <dt-bindings/thermal/thermal.h>
#include <dt-bindings/scu/scu-leo.h>
 
/ {
	model = "leo";
	compatible = "arm,faraday-soc";
	#address-cells = <1>;
	#size-cells = <1>;
	interrupt-parent = <&gic>;
	
	aliases {
		serial0 = &uart0;
		serial1 = &uart1;
		serial2 = &uart2;
		serial3 = &uart3;
		serial4 = &uart4;
		serial5 = &uart5;
		serial6 = &uart6;
		serial7 = &uart7;
		serial8 = &uart8;
		serial9 = &uart9;
		dmac030_0 = &dma0;
		dmac030_1 = &dma1;
	};
	
	firmware {
		optee@11110000{
		compatible = "linaro,optee-tz";
		method = "smc";
		};
	};
/*
	psci {
		compatible	= "arm,psci";
		method		= "smc";
		cpu_suspend	= <0x84000001>;
		cpu_off		= <0x84000002>;
		cpu_on		= <0x84000003>;
	};
*/
	cpus {
		#address-cells = <1>;
		#size-cells = <0>;
		
		cpu0: cpu@0 {
			compatible = "arm,cortex-a7";
			device_type = "cpu";
			clock-frequency = <800000000>;
			reg = <0x0>;
			/*enable-method = "psci";*/
		};
		cpu1: cpu@1 {
			compatible = "arm,cortex-a7";
			device_type = "cpu";
			clock-frequency = <800000000>;
			reg = <0x1>;
			/*enable-method = "psci";*/
		};
		cpu2: cpu@2 {
			compatible = "arm,cortex-a7";
			device_type = "cpu";
			clock-frequency = <800000000>;
			reg = <0x2>;
			/*enable-method = "psci";*/
		};
		cpu3: cpu@3 {
			compatible = "arm,cortex-a7";
			device_type = "cpu";
			clock-frequency = <800000000>;
			reg = <0x3>;
			/*enable-method = "psci";*/
		};
	};
	
	memory@80000000 {
		device_type = "memory";
		reg = <0x80000000 0x80000000>;
	};
	
	timer {
		compatible = "arm,armv7-timer";
		clock-frequency = <800000000>;  /* kernel default cpu frequency */
		interrupts = <GIC_PPI 13 (GIC_CPU_MASK_SIMPLE(4) | IRQ_TYPE_LEVEL_LOW)>,
		             <GIC_PPI 14 (GIC_CPU_MASK_SIMPLE(4) | IRQ_TYPE_LEVEL_LOW)>,
		             <GIC_PPI 11 (GIC_CPU_MASK_SIMPLE(4) | IRQ_TYPE_LEVEL_LOW)>,
		             <GIC_PPI 10 (GIC_CPU_MASK_SIMPLE(4) | IRQ_TYPE_LEVEL_LOW)>;
		arm,cpu-registers-not-fw-configured;
		clocks = <&cpu>;
		clock-names = "cpu";
		status = "okay";
	};
	
	chosen {
		bootargs = "mem=2048M console=ttyS0,115200 earlycon=uart8250,mmio32,0x54e00000 root=/dev/mmcblk0p2 rw init=/sbin/init rootfstype=ext4";
#if 0
                bootargs = "mem=2048M console=ttyS0,115200 earlycon=uart8250,mmio32,0x54e00000 root=/dev/mmcblk0p2 rw init=/linuxrc rootfstype=ext4";
               bootargs = "root=/dev/nfs   nfsroot=192.168.0.2:/home/niubin/rootfs,vers=3 ip=192.168.0.80:192.168.0.2:192.168.0.1:255.255.255.0::eth0:off init=/linuxrc mem=2048M console=ttyS0,115200 earlycon=uart8250,mmio32,0x54e00000";
#endif
	};
	
	clocks {
		compatible = "faraday,leoevb-clk";
		
		osc0: osc0-25mhz {
			#clock-cells = <0>;
			compatible = "leo,osc0";
			clock-frequency = <25000000>;
			clock-output-names = "osc0";
		};
		
		rosc0: rosc0-120mhz {
			#clock-cells = <0>;
			compatible = "leo,rosc0";
			clock-frequency = <120000000>;
			clock-output-names = "rosc0";
		};
		
		rosc0_div4: rosc0_div4 {
			#clock-cells = <0>;
			compatible = "leo,rosc0_div4";
			clock-mult = <1>;
			clock-div = <4>;
			clock-output-names = "rosc0_div4";
			clocks = <&rosc0>;
		};
		
		rosc1: rosc1-32768hz {
			#clock-cells = <0>;
			compatible = "leo,rosc1";
			clock-frequency = <32768>;
			clock-output-names = "rosc1";
		};
		
		audio: audio-12288khz {
			#clock-cells = <0>;
			compatible = "leo,audio";
			clock-frequency = <12288000>;
			clock-output-names = "audio";
		};
		
		pll0: pll0 {
			#clock-cells = <0>;
			compatible = "leo,pll0";
			clock-div = <1>;
			clock-output-names = "pll0";
			clocks = <&osc0>;
		};
		
		pll1: pll1 {
			#clock-cells = <0>;
			compatible = "leo,pll1";
			clock-div = <1>;
			clock-output-names = "pll1";
			clocks = <&osc0>;
		};
		
		
		pll1_div4: pll1_div4 {
			#clock-cells = <0>;
			compatible = "leo,pll1_div4";
			clock-mult = <1>;
			clock-div = <4>;
			clock-output-names = "pll1_div4";
			clocks = <&pll1>;
		};
		
		pll2: pll2 {
			#clock-cells = <0>;
			compatible = "leo,pll2";
			clock-div = <1>;
			clock-output-names = "pll2";
			clocks = <&osc0>;
		};
		
		pll2_div5: pll2_div5 {
			#clock-cells = <0>;
			compatible = "leo,pll2_div5";
			clock-mult = <1>;
			clock-div = <5>;
			clock-output-names = "pll2_div5";
			clocks = <&pll2>;
		};
		
		pll2_div50: pll2_div50 {
			#clock-cells = <0>;
			compatible = "leo,pll2_div50";
			clock-mult = <1>;
			clock-div = <50>;
			clock-output-names = "pll2_div50";
			clocks = <&pll2>;
		};
		
		pll3: pll3 {
			#clock-cells = <0>;
			compatible = "leo,pll3";
			clock-div = <16>;
			clock-output-names = "pll3";
			clocks = <&pll2_div50>;
		};
		
		pll4: pll4 {
			#clock-cells = <0>;
			compatible = "leo,pll4";
			clock-div = <2>;
			clock-output-names = "pll4";
			clocks = <&osc0>;
		};
		
		pll5: pll5 {
			#clock-cells = <0>;
			compatible = "leo,pll5";
			clock-div = <8>;
			clock-output-names = "pll5";
			clocks = <&pll2_div50>;
		};
		
		pll6: pll6 {
			#clock-cells = <0>;
			compatible = "leo,pll6";
			clock-div = <1>;
			clock-output-names = "pll6";
			clocks = <&osc0>;
		};
		
		pll7: pll7 {
			#clock-cells = <0>;
			compatible = "leo,pll7";
			clock-div = <2>;
			clock-output-names = "pll7";
			clocks = <&osc0>;
		};
		
		fastboot: fastboot {
			#clock-cells = <0>;
			compatible = "leo,fastboot";
			clock-output-names = "fastboot";
			clocks = <&rosc0>, <&osc0>;
		};
		
		fastboot_div2: fastboot_div2 {
			#clock-cells = <0>;
			compatible = "leo,fastboot_div2";
			clock-output-names = "fastboot_div2";
			clock-mult = <1>;
			clock-div = <2>;
			clocks = <&fastboot>;
		};
		
		ahb: ahb {
			#clock-cells = <0>;
			compatible = "leo,ahb";
			clock-mult = <1>;
			clock-div = <4>;
			clock-output-names = "AHB";
			clocks = <&pll1>, <&fastboot_div2>;
		};
		
		hclk: hclk {
			#clock-cells = <0>;
			compatible = "leo,hclk";
			clock-mult = <1>;
			clock-div = <4>;
			clock-output-names = "hclk";
			clocks = <&pll1>, <&fastboot_div2>;
		};
		
		apb: apb {
			#clock-cells = <0>;
			compatible = "leo,apb";
			clock-mult = <1>;
			clock-div = <8>;
			clock-output-names = "APB";
			clocks = <&fastboot>, <&pll6>;
		};
		
		pclk: pclk {
			#clock-cells = <0>;
			compatible = "leo,pclk";
			clock-mult = <1>;
			clock-div = <8>;
			clock-output-names = "pclk";
			clocks = <&fastboot>, <&pll6>;
		};
		
		cpu: cpu {
			#clock-cells = <0>;
			compatible = "leo,cpu";
			mux-reg = <0x30>;
			mux-shift = <6>;
			mux-mask = <1>;
			div-reg = <0x30>;
			div-shift = <4>;
			div-width = <3>;
			clock-output-names = "cpu";
			clocks = <&pll0>, <&fastboot>;
		};
		
		ddrmclk: ddrmclk {
			#clock-cells = <0>;
			compatible = "leo,ddrmclk";
			clock-mult = <1>;
			clock-div = <1>;
			clock-output-names = "ddrmclk";
			clocks = <&pll4>;
		};
		
		spiclk: spiclk {
			#clock-cells = <0>;
			compatible = "leo,spiclk";
			clock-output-names = "spiclk";
			clocks = <&fastboot>, <&pll1>;
		};
		
		sspclk: sspclk {
			#clock-cells = <0>;
			compatible = "leo,sspclk";
			clock-output-names = "sspclk";
			clocks = <&fastboot>, <&pll1>;
		};
		
		sspclk_i2s: sspclk_i2s {
			#clock-cells = <0>;
			compatible = "leo,sspclk_i2s";
			clock-output-names = "sspclk_i2s";
			clocks = <&sspclk>, <&audio>;
		};
		
		sspclk_i2s_1: sspclk_i2s_1 {
			#clock-cells = <0>;
			compatible = "leo,sspclk_i2s_1";
			clock-output-names = "sspclk_i2s_1";
			clocks = <&sspclk>, <&audio>;
		};
		
		sdclk: sdclk {
			#clock-cells = <0>;
			compatible = "leo,sdclk";
			clock-output-names = "sdclk";
			clocks = <&fastboot_div2>, <&pll7>;
		};
		
		lcclk: lcclk {
			#clock-cells = <0>;
			compatible = "leo,lcclk";
			clock-output-names = "lcclk";
			clock-mult = <1>;
			clock-div = <2>;
			clocks = <&pll5>;
		};
		
		uart_uclk_src: uart_uclk_src {
			#clock-cells = <0>;
			compatible = "leo,uart_uclk_src";
			clock-mult = <100>;
			clock-div = <425>;
			clock-output-names = "uart_uclk_src";
			clocks = <&pll1_div4>;
		};
		
		uart_uclk_30m: uart_uclk_30m {
			#clock-cells = <0>;
			compatible = "leo,uart_uclk_30m";
			clock-mult = <1>;
			clock-output-names = "uart_uclk_30m";
			clocks = <&rosc0_div4>, <&uart_uclk_src>;
		};
		
		uart_uclk: uart_uclk {
			#clock-cells = <0>;
			compatible = "leo,uart_uclk";
			clock-mult = <1>;
			clock-output-names = "uart_uclk";
			clocks = <&uart_uclk_30m>, <&pll2_div5>;
		};
		
		irdaclk: irdaclk {
			#clock-cells = <0>;
			compatible = "leo,irda";
			clock-mult = <1>;
			clock-div = <5>;
			clock-output-names = "irda";
			clocks = <&pll2>;
		};
		
		gmacclk: gmacclk {
			#clock-cells = <0>;
			compatible = "leo,gmacclk";
			clock-mult = <1>;
			clock-div = <20>;
			clock-output-names = "gmacclk";
			clocks = <&pll1>;
		};
	};


	pinctrl {
		compatible = "ftscu010-pinmux", "pinctrl-leo";
		reg = <0x28000000 0xA000>;

		scu010,modex = <
			LEO_Mode_X_GPIO0_26
			LEO_Mode_X_GPIO0_25
#if 0
			LEO_Mode_X_GPIO0_24   
#endif 
			LEO_Mode_X_GPIO0_23
			LEO_Mode_X_GPIO0_22
#if 0
			LEO_Mode_X_GPIO0_21
#endif
			LEO_Mode_X_GPIO0_20
			LEO_Mode_X_GPIO0_19
#if 0
			LEO_Mode_X_GPIO0_18
			LEO_Mode_X_GPIO0_17
#endif
			LEO_Mode_X_GPIO0_3
			LEO_Mode_X_GPIO0_2
		>;
		
		pinctrl_ftcan010: pinctrl_ftcan010 {
			scu010,function = <LEO_MUX_FTCAN010>;
			scu010,pins = <
			#if 0
				LEO_PIN_X_LC_PCLK
				LEO_PIN_X_LC_VS
			#elif 0
				LEO_PIN_X_RMII0_TXD_0
				LEO_PIN_X_RMII0_TXD_1
			#else
				LEO_PIN_X_UART5_RX
				LEO_PIN_X_UART5_TX
			#endif
			>;
		};
		
		pinctrl_ftcan010_1: pinctrl_ftcan010_1 {
			scu010,function = <LEO_MUX_FTCAN010_1>;
			scu010,pins = <
			#if 0
				LEO_PIN_X_LC_DE
				LEO_PIN_X_LC_HS
			#elif 0
				LEO_PIN_X_RMII0_RX_CRSDV
				LEO_PIN_X_RMII0_TX_EN
			#else
				LEO_PIN_X_UART8_RX
				LEO_PIN_X_UART8_TX
			#endif
			>;
		};
		
		pinctrl_ftgmac030_mii: pinctrl_ftgmac030_mii {
			scu010,function = <LEO_MUX_FTGMAC030_MII>;
			scu010,pins = <
				LEO_PIN_X_MII_COL
				LEO_PIN_X_MII_CRS
				LEO_PIN_X_MII_MDC
				LEO_PIN_X_MII_MDIO
				LEO_PIN_X_MII_RXD_0
				LEO_PIN_X_MII_RXD_1
				LEO_PIN_X_MII_RXD_2
				LEO_PIN_X_MII_RXD_3
				LEO_PIN_X_MII_RX_CK
				LEO_PIN_X_MII_RX_DV
				LEO_PIN_X_MII_RX_ER
				LEO_PIN_X_MII_TXD_0
				LEO_PIN_X_MII_TXD_1
				LEO_PIN_X_MII_TXD_2
				LEO_PIN_X_MII_TXD_3
				LEO_PIN_X_MII_TX_CK
				LEO_PIN_X_MII_TX_EN
				LEO_PIN_X_MII_TX_ER
			>;
		};
		
		pinctrl_ftgmac030_rgmii: pinctrl_ftgmac030_rgmii {
			scu010,function = <LEO_MUX_FTGMAC030_RGMII>;
			scu010,pins = <
				LEO_PIN_X_RGMII_GTX_CK
				LEO_PIN_X_RGMII_MDC
				LEO_PIN_X_RGMII_MDIO
				LEO_PIN_X_RGMII_RXCTL
				LEO_PIN_X_RGMII_RXD_0
				LEO_PIN_X_RGMII_RXD_1
				LEO_PIN_X_RGMII_RXD_2
				LEO_PIN_X_RGMII_RXD_3
				LEO_PIN_X_RGMII_RX_CK
				LEO_PIN_X_RGMII_TXCTL
				LEO_PIN_X_RGMII_TXD_0
				LEO_PIN_X_RGMII_TXD_1
				LEO_PIN_X_RGMII_TXD_2
				LEO_PIN_X_RGMII_TXD_3
			>;
		};
		
		pinctrl_ftgmac030_rmii: pinctrl_ftgmac030_rmii {
			scu010,function = <LEO_MUX_FTGMAC030_RMII>;
			scu010,pins = <
				LEO_PIN_X_RMII0_MDC
				LEO_PIN_X_RMII0_MDIO
				LEO_PIN_X_RMII0_REF_CLK
				LEO_PIN_X_RMII0_RXD_0
				LEO_PIN_X_RMII0_RXD_1
				LEO_PIN_X_RMII0_RX_CRSDV
				LEO_PIN_X_RMII0_RX_ER
				LEO_PIN_X_RMII0_TXD_0
				LEO_PIN_X_RMII0_TXD_1
				LEO_PIN_X_RMII0_TX_EN
			>;
		};
		
		pinctrl_ftgpio010: pinctrl_ftgpio010 {
			scu010,schmitt-trigger;
			scu010,function = <LEO_MUX_FTGPIO010>;
			scu010,pins = <
				LEO_PIN_X_LC_PCLK        //GPIO0[0] - Mode5 / Mode7
				//LEO_PIN_X_UART5_TX       //GPIO0[0] - Mode3 / GPIO0[2] - ModeX
				//LEO_PIN_X_UART5_RX       //GPIO0[1] - Mode3 / GPIO0[3] - ModeX   
				//LEO_PIN_X_LC_VS          //GPIO0[1] - Mode5 / Mode7
				//LEO_PIN_X_LC_HS          //GPIO0[2] - Mode5 / Mode7
				//LEO_PIN_X_LC_DE          //GPIO0[3] - Mode5 / Mode7
