			>;
		};
		
		pinctrl_ftgpio010_1: pinctrl_ftgpio010_1 {
			scu010,schmitt-trigger;
			scu010,function = <LEO_MUX_FTGPIO010_1>;
			scu010,pins = <
				//LEO_PIN_X_MII_MDIO       //GPIO1[0] - Mode7 / GPIO1[26] - Mode1 / Mode3
				//LEO_PIN_X_EMMC1_D7       //GPIO1[0] - Mode1 / Mode3 / Mode4 / Mode5 / Mode7
				//LEO_PIN_X_MII_RX_CK      //GPIO1[1] - Mode7
