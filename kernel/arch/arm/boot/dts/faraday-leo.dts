				//LEO_PIN_X_LC_DATA_4      //GPIO0[8] - Mode5 / Mode7
				//LEO_PIN_X_LC_DATA_5      //GPIO0[9] - Mode5 / Mode7 / GPIO0[21] - ModeX
				//LEO_PIN_X_UART8_TX       //GPIO0[9] - Mode3  
				//LEO_PIN_X_MII_RX_CK      //GPIO0[9] - Mode1 / Mode3 / Mode4
				//LEO_PIN_X_LC_DATA_6      //GPIO0[10] - Mode5 / Mode7 / GPIO0[17] - ModeX
				//LEO_PIN_X_MII_RXD_2      //GPIO0[10] - Mode1 / Mode3 / Mode4
				//LEO_PIN_X_UART8_RX       //GPIO0[10] - Mode3
				//LEO_PIN_X_MII_RXD_3      //GPIO0[11] - Mode1 / Mode3 / Mode4) 
				//LEO_PIN_X_UART8_RX_h     //GPIO0[11] - Mode3 
				//LEO_PIN_X_LC_DATA_7      //GPIO0[11] - Mode5 / Mode7
				//LEO_PIN_X_LC_DATA_8      //GPIO0[12] - Mode5 / Mode7
				//LEO_PIN_X_MII_TX_CK      //GPIO0[12] - Mode1 / Mode3 /Mode4 
				//LEO_PIN_X_LC_DATA_9      //GPIO0[13] - Mode5 / Mode7
				//LEO_PIN_X_MII_TX_ER      //GPIO0[13] - Mode1 / Mode3 / Mode4
				//LEO_PIN_X_LC_DATA_10     //GPIO0[14] - Mode5 / Mode7
				//LEO_PIN_X_MII_TXD_2      //GPIO0[14] - Mode1 / Mode3 / Mode4
				//LEO_PIN_X_MII_TXD_3      //GPIO0[15] - Mode1 / Mode3 / Mode4
				//LEO_PIN_X_LC_DATA_11     //GPIO0[15] - Mode5 / Mode7
				//LEO_PIN_X_LC_DATA_12     //GPIO0[16] - Mode5 / Mode7
				//LEO_PIN_X_MII_CRS        //GPIO0[16] - Mode1 / Mode3 / Mode4
