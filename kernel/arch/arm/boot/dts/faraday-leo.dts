				//LEO_PIN_X_I2S0_TXD       //GPIO0[18] - ModeX 
				//LEO_PIN_X_LC_DATA_15     //GPIO0[19] - Mode5 / Mode7
				//LEO_PIN_X_UART2_NRTS     //GPIO0[19] - ModeX 
				//LEO_PIN_X_LC_DATA_16     //GPIO0[20] - Mode5 / Mode7
				//LEO_PIN_X_UART2_NCTS     //GPIO0[20] - ModeX
				//LEO_PIN_X_LC_DATA_17     //GPIO0[21] - Mode5 / Mode7
				//LEO_PIN_X_LC_DATA_18     //GPIO0[22] - Mode5 / Mode7
				//LEO_PIN_X_I2S0_RXD       //GPIO0[22] - ModeX /GPIO1[12] - Mode2
				//LEO_PIN_X_I2S0_SCLK      //GPIO0[23] - ModeX /GPIO1[13] - Mode2
				//LEO_PIN_X_LC_DATA_19     //GPIO0[23] - Mode5 / Mode7
				//LEO_PIN_X_LC_DATA_20     //GPIO0[24] - Mode5 / Mode7
				//LEO_PIN_X_MII_TX_EN      //GPIO0[24] - Mode7
				//LEO_PIN_X_LC_DATA_21     //GPIO0[25] - Mode5 / Mode7
				//LEO_PIN_X_I2S0_FS        //GPIO0[25] - ModeX
