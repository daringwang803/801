	soc {
		#address-cells = <1>;
		#size-cells = <1>;
		compatible = "simple-bus";
		ranges;
		
		gic: interrupt-controller@70000000 {
			compatible = "arm,cortex-a7-gic";
			interrupt-controller;
			#interrupt-cells = <3>;
			reg = <0x70001000 0x1000>,
			      <0x70002000 0x2000>,
			      <0x70004000 0x2000>,
			      <0x70006000 0x2000>;
		};

		cpufreq0: cpufreq@28000000 {
			compatible = "faraday,faraday-cpufreq";
			interrupt-parent = <&gic>;
			reg = <0x28000000 0xA000>,
			      <0x28900000 0x1000>,
			      <0x54e00000 0x1000>;
			reg-names = "sysc", "ddrc", "uart";
			interrupts = <GIC_SPI 0 IRQ_TYPE_LEVEL_HIGH>;
			clocks = <&cpu>;
			clock-names = "cpu";
			operating-points = <
				/* KHz	  uV */
				100000 1100000
				200000 1100000
				400000 1100000
				800000 1100000
			>;
			status = "okay";
		};

		efuse0@22000000 {
	                compatible = "faraday,ftc-efuse";
        	        interrupt-parent = <&gic>;
                	reg = <0x22000000 0x10000>;
	                efuse-size = <512>;   /* 4096 bits */
        	        status = "okay";
        	};

		uart0: uart@54e00000 {
			compatible = "of_serial", "ns16550a";
			interrupt-parent = <&gic>;
			clock-frequency = <58823529>;
			reg = <0x54e00000 0x1000>;
			reg-offset = <0>;
			reg-shift = <2>;
			reg-io-width = <4>;
			no-loopback-test;
			interrupts = <GIC_SPI 45 IRQ_TYPE_LEVEL_HIGH>;
			serial = <0>;
			status = "okay";
		};
		
		uart1: uart@54f00000 {
			compatible = "of_serial", "ns16550a";
			interrupt-parent = <&gic>;
			clock-frequency = <58823529>;
			reg = <0x54f00000 0x1000>;
			reg-offset = <0>;
			reg-shift = <2>;
			reg-io-width = <4>;
			no-loopback-test;
			interrupts = <GIC_SPI 47 IRQ_TYPE_LEVEL_HIGH>;
			serial = <1>;
			status = "okay";
		};
		
		uart2: uart@55000000 {
			compatible = "of_serial", "ns16550a";
